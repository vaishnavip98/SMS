@{
    ViewData["Title"] = "MainPage";
}
<div>
    <div class="topnav">

        <h1>Welcome</h1>

        <div class="lb">
            <div class="stxt">
                <input type="search" class="p-1" placeholder="Search" aria-label="Search" aria-describedby="search-addon" />
                <button type="button" class="btn btn-outline-primary p-1">search</button>
            </div>
            <button type="button" class="btn btn-outline">LogOut</button>
        </div>

    </div>
    <div class="page1">
        <div class="leftbar">
            <div class="img">
                <img src="data:image/png;base64," />
            </div>
            <div>
                <div class="sbtn">
                    <input class="file-upload-input i2" type='file' accept="image/*" />
                    <button>Upload</button>
                </div>
            </div>
            <div class="pbtn">
                <button>Home</button>
                <button>Gallery</button>
                <button>My Friend</button>
                <button>My Account</button>
                <button>Send SMS</button>
                <button>My Message</button>
                <button>Change Password</button>
            </div>
        </div>
        <div>
                <p><label for="txtarea">Share your good thoughts to world.</label></p>
                <textarea id="txtarea" name="txtarea" rows="4" cols="50"></textarea>
                <input type="submit" value="Post" />
        </div>
        <div class="rightbar">
            <p class="fw-bold freq">People You May Know</p>
        </div>
    </div>
</div>